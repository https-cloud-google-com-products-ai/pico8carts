function make_bg()
 return {
  x=0,
  y=0,
  update=function(t,s)
  
  
  end,
  draw=function(t)
  
   local phase = ((g_tick % 60)
     / 60)
   --[[
   if phase < 0.2
     or phase > 0.8 then
    pal(7, 2)
    map(0,0,-2,-2,8,8)
    map(0,0,2,2,8,8)
    map(0,0,-2,2,8,8)
    map(0,0,2,-2,8,8)
    map(0,0,0,2,8,8)
    map(0,0,0,-2,8,8)
    map(0,0,2,0,8,8)
    map(0,0,-2,0,8,8)
   end
   --]]
   --if phase < 0.4
   --  or phase > 0.6 then
    pal(7, 1)
    pal(13, 1)
    
    map(0,0,-1,-1,8,8)
    map(0,0,1,1,8,8)
    map(0,0,-1,1,8,8)
    map(0,0,1,-1,8,8)
   --end
   
   if g_tick % 30 > 14 then
    pal(7,8)
    pal(13,14)
   else
    pal(7,14)
    pal(13,8)
   end
   
   map(0,0,0,0,8,8) 
   pal()
  end
 }
end

