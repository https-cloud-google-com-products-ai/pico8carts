function make_wave()
 return {
  x = 0,
  y = 0,
  update=function(t,s)
  
  
  
  end,
  draw=function(t)
   local b = 0x6000
   
   local a = (sin(g_tick / 50)
       + 1) * 3
   --local a = 1.5
   local yphase = 20-- (sin(g_tick/200)+1) * 30
   
   for y = 0, 127 do
    local o = flr(sin(
        y/yphase + g_tick/10) * a)
    
    local len = 64
    local src = b
    local dst = b + o
    
    
    memcpy(b, b + o, 64)
    b = b + 64
   
   end
  
  end
 }
end



