function make_maze()
 local r = {
  x=0,
  y=0,
  cx=0,
  cy=0,
  sx=2,
  sy=2,
  state=0, --0, 1 cw, 2 ccw
  update=function(t,s)
   if t.state == 0 then
    if btnn(4) then
     t.state = 1
     t.rt = g_tick
    elseif btnn(5) then
     t.state = 2
     t.rt = g_tick
    elseif btnn(0) then
     if t.cx > 0 then
      t.cx = t.cx - 1
     end
    elseif btnn(1) then
     if t.cx < t.sx - 1 then
      t.cx = t.cx + 1
     end
    elseif btnn(2) then
     if t.cy > 0 then
      t.cy = t.cy - 1
     end
    elseif btnn(3) then
     if t.cy < t.sy - 1 then
      t.cy = t.cy + 1
     end
    end
    
    
   else
    if elapsed(t.rt) >= 10 then
     --todo, rotate the block 
     
     local cell =
       t.b[t.cy+1][t.cx+1]
     
     local o = -1
     if t.state == 2 then
      o = 1
     end
     cell.r = (cell.r + o) % 4
     
     t.rt = nil
     t.state = 0
    end
   end
   
  end,
  draw=function(t)
   
   local flipcol = (
     g_tick % 30 > 14)
   
   t.basepal(flipcol)
   
   for y = 1, #t.b do
    local row = t.b[y]
    for x = 1, #row do
     local cell = row[x]
      
      local mx = cell.r*8
      local my = cell.m*8
      local sx = (x-1)*64
      local sy = (y-1)*64
      
      
      if t.cx == x - 1
        and t.cy == y  -1 then
       
       if t.state == 0 then
        pal(7, 6)
        pal(13, 6) 
       
        for xy in all({
          {-1,-1},
          {1,1},
          {-1,1},
          {1,-1}}) do
         map(mx,my,sx+xy[1],
           sy+xy[2],8,8)
        end
       
        t.basepal(flipcol)
        map(mx,my,sx,sy,8,8)
       else
        local segs =
          getmapsegments(
            mx,my,8,8)
        
        
        local ang =
          elapsed(t.rt) / 10 *
            90
        
        if t.state == 2 then
         ang = ang * -1
        end
        
        local lc = 2
        
        
        pushc(-sx,-sy)
        local p1 = {x=-32,y=-32}
        local p2 = {x=32,y=32}
        for s in all(segs) do
         local v1 = vecadd({
           x=s[1],y=s[2]}, p1)
         local v2 = vecadd({
           x=s[3],y=s[4]}, p1)
         
         v1 = vecadd(p2,
           vecrot(v1, ang))
         v2 = vecadd(p2,
           vecrot(v2, ang))
      
         line(v1.x,v1.y,
           v2.x,v2.y, lc)
        end
        popc()
        
       end
      else
       map(mx,my,sx,sy,8,8)
      end
      
      
    end
   end
   
   pal()   
  end,
  basepal=function(flipcol)
   if flipcol then
    pal(7,8)
    pal(13,14)
   else
    pal(7,14)
    pal(13,8)
   end
  end
 }
 
 local b = {}
 r.b = b
 
 for y = 1, r.sy do
  local row = {}
  add(b, row)
  for x = 1, r.sx do
   local cell = {}
   add(row, cell)
   cell.m = flr(rnd(g_maxcells))
   cell.r = flr(rnd(4))
  end
 end
 
 return r
end

