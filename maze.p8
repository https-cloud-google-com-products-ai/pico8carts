function make_player()
 return {
  x=0,
  y=0,
  update=function(t,s)
  
  end,
  draw=function(t)
   local s = flr(
       (g_tick % 8) / 2)
   
   
   for i = 0, 15 do
    pal(i,5) 
   end
   spr(32+s,5,5)
   spr(32+s,3,3)
   
   pal()
   spr(32+s,4,4) 
  end
 
 }
end
