pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
function make_board(w, h, x, y, p)
 local b = {}  
 b.w = w
 b.h = h 
 b.t = {}
 for i = 1, h do
  local r = {}
  for j = 1, w do
   r[j] = {}
   local n = flr(rnd(5) + 1)
   if j > 2 then
    if n == r[j-1].t and n == r[j-2].t then
     n += 1
     if n > g.l_b then
      n = g.f_b
     end
    end
   end
   r[j].t = n
  end
  b.t[i] = r
 end  
 b.cx = 0
 b.cy = 0
 b.x = x
 b.y = y
 b.p = p or 0
 return b
end

function update_board(b)
 local m = false
 if btnp(0, b.p) then
  if b.cx > 0 then
   b.cx -= 1
   m = true
  end
 end
 if btnp(1, b.p) then
  if b.cx < b.w - 2 then
   b.cx += 1
   m = true
  end
 end
 if btnp(2, b.p) then
  if b.cy > 0 then
   b.cy -= 1
   m = true
  end
 end
 if btnp(3, b.p) then
  if b.cy < b.h - 1 then
   b.cy += 1
   m = true
  end
 end
 if m then sfx(0) end
 
 --todo: multiframe flip
 if btnp(4, b.p) then
  local x = b.cx+1
  local y = b.cy+1
  local v = b.t[y][x]
  b.t[y][x] = b.t[y][x+1]
  b.t[y][x+1] = v
  sfx(1)
 end
 
 scan_board(b)
end

function scan_board(b)
 local k = g.tick
 local ms = {}

 for h = 1, b.h do
  local r = b.t[h]
  for w = 1,b.w do
   local t = r[w]

   if t.m ~= k then
    t.m = nil
   end

   if t.t > 0 and not t.m then
    if w < b.w-1 then
     local wc = 1
     for i=(w+1),b.w do
      if t.t == r[i].t then
       wc+=1
      else
       break
      end
     end
     if wc > 2 then
      for i=w,w+(wc-1) do
       add(ms, r[i])
      end
     end 
    end

    if h < b.h-1 then
     local hc = 1
     for i=(h+1),b.h do
      if t.t == b.t[i][w].t then
       hc+=1
      else
       break
      end
     end
     if hc > 2 then
      for i=h,h+(hc-1) do
       add(ms, b.t[i][w])
      end
     end

    end
   end
  end
 end
 
 for _,t in pairs(ms) do
  t.m = k
 end
 
end

function draw_board(b)
 pushc(-b.x, -b.y)
 for h = 1, b.h do
  local r = b.t[h]
  for w = 1, b.w do
   local s = r[w].t
   if s > 0 then
    if r[w].m then
     s+=16
    end
    spr(s,(w-1)*9,(h-1)*9)
   end
  end
 end
 local x = b.cx*9
 local y = b.cy*9
 draw_curs(x, y, g.tick%30 < 15)
 popc() 
end

function draw_curs(x, y, grow)
 local c,l,p=7,1,0
 if grow then
  l,p=2,1
 end
 pushc(-(x-p),-(y-p))
 line(0,0,l,0,c)
 line(0,0,0,l,c)
 popc() 
 pushc(-(x-p),-(y+7+p))
 line(0,0,l,0,c)
 line(0,0,0,-l,c)
 popc()
 pushc(-(x+16+p),-(y-p))
 line(0,0,-l,0,c)
 line(0,0,0,l,c)
 popc()
 pushc(-(x+16+p),-(y+7+p))
 line(0,0,-l,0,c)
 line(0,0,0,-l,c)
 popc()
 pushc(-(x+8),-(y-p))
 line(-(1+p),0,1+p,0,c)
 line(0,0,0,l,c)
 popc()
 pushc(-(x+8),-(y+7+p))
 line(-(1+p),0,1+p,0,c)
 line(0,0,0,-l,c)
 popc()
end
--
function elapsed(t)
 if g.tick>=t then
  return g.tick - t
 end
 return 32767-t+g.tick
end

function pushc(x, y)
 local l=g.cs[#g.cs] or {0,0}
 local n={l[1]+x,l[2]+y}
 add(g.cs, n)
 camera(n[1], n[2])
end

function popc()
 local len = #g.cs
 g.cs[len] = nil
 len -= 1
 if len > 0 then
  local xy=g.cs[len]
  camera(xy[1],xy[2])
 else
  camera()
 end
end
--
function _update()
 if g.tick<32767then
  g.tick+=1
 else
  g.tick=0
 end
 foreach(g.bs,update_board)
end

function _draw()
 cls()
 foreach(g.bs,draw_board)
 print('cpu:'..(flr(stat(1)*100))..'%')
end

function _init()
 g = {}
 g.e_b = 0
 g.f_b = 1
 g.l_b = 5
 g.cs = {}
 g.bs = {}
 add(g.bs, make_board(6,12,1,8))
 add(g.bs, make_board(6,12,74,8,1))  
 g.tick = 0
end

